// Glycerol Transport

reaction GlycerolTransport(glycerol{out} -> glycerol{in})
specification
{
    float cgt = 0.5 M;
    G6P' = glycerol{out}/glycerol{in} * cgt;
}

glycerol{out}[:] = 0.15e-6 M;
glycerol{in} = 8.8e-3 M;


