// Building Block Consumption

import * from MetaboliteConcentrations;

import EcoliInfo;

reaction dNTPConsumptionByDNAReplication(dATP + dCTP + dGTP + dTTP + 3 ATP -> 3 ADP) // for cell division
specification
{
    dATP' = EcoliInfo.DNAReplicationRate * EcoliInfo.C2M / 4;
}

reaction AAConsumptionByProteinSynthesis(glutamine + 10 ATP -> 10 ADP) // for cell division
specification
{
    glutamine' = EcoliInfo.ProteinSynthesisRate * EcoliInfo.C2M / 1;
}

