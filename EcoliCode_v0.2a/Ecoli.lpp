// E. coli Code v0.2

// Check how to import concentrations
import * from MetaboliteConcentrations;
import MetaboliteConcentrations;

import Metabolism.Metabolism;
import CellDivision.BinaryFission;
import CellMotility.CellMotility;
import GeneticInfoProcessing.Transcription.Transcription;
import GeneticInfoProcessing.Translation.Translation;

organism Ecoli
{
    Metabolism = Metabolism.Metabolism.Metabolism;
    CellDivision = CellDivision.BinaryFission.BinaryFission;
    Transcription = GeneticInfoProcessing.Transcription.Transcription.Transcription;
    Translation = GeneticInfoProcessing.Translation.Translation.Translation;

    membrane CellMembrane
    {
        cytosol Cytosol
        {
            G6P[:] = MetaboliteConcentrations.G6P;
        }
    }
}

organism Ecoli_Chemotaxis
{
    Metabolism = Metabolism.Metabolism.Metabolism;
    CellDivision = CellDivision.BinaryFission.BinaryFission;
    CellMotility = CellMotility.CellMotility.CellMotility;
    Transcription = GeneticInfoProcessing.Transcription.Transcription.Transcription;
    Translation = GeneticInfoProcessing.Translation.Translation.Translation;

    membrane CellMembrane
    {
        cytosol Cytosol
        {
            G6P[:] = MetaboliteConcentrations.G6P;
        }
    }
}

Ecoli = 1;
Ecoli_Chemotaxis = 1;
