reaction R4(Gd + Gbg --> G, k=6.0221409e+23);

R = 6.6421561142815505e-21;
L[:] = 0;
L[100:700] = 1e-9;
G = 1.6605390285703877e-20;

// temporary simulation parameters
SimSteps = 10000, SimRes = 10;
