// NADH Oxidative Phosphorylation

import EcoliInfo;
import * from MetaboliteConcentrations;

pathway NADH_OxidativePhosphorylation(NADH + 2.5 ADP -> NAD + 2.5 ATP)
specification
{
    float Init_NADH = 83.2nM;
    ATP' = NADH / (Init_NADH + NADH) * EcoliInfo.EGM_OxidativePhosphorylation_NADH_Sec;
}
