// dNTP Synthesis

from MetaboliteConcentrations import *;

import FolicAcidMetabolism;
import PRPPSynthesis;
import PurineMetabolism;
import PyrimidineMetabolism;

pathway dNTPSynthesis
{
    membrane CellMembrane
    {
        cytosol Cytosol
        {
            DHFSynthesis = FolicAcidMetabolism.DHFSynthesis;
            THFSynthesisByFolA = FolicAcidMetabolism.THFSynthesisByFolA;
            FiveTenMethyleneTHFSynthesisByGlyA = FolicAcidMetabolism.FiveTenMethyleneTHFSynthesisByGlyA;
            TenFormylTHFSynthesis = FolicAcidMetabolism.TenFormylTHFSynthesis;
            
            PRPPSynthesis = PRPPSynthesis.PRPPSynthesis;

            GARSynthesis = PurineMetabolism.GARSynthesis;
            FGARSynthesis = PurineMetabolism.FGARSynthesis;
            FGAMSynthesis = PurineMetabolism.FGAMSynthesis;
            PurineSynthesis = PurineMetabolism.PurineSynthesis;

            dCTPSynthesis = PyrimidineMetabolism.dCTPynthesis;
            dUTPSynthesis = PyrimidineMetabolism.dUTPynthesis;
            dTTPSynthesisByThyA = PyrimidineMetabolism.dTTPSynthesisByThyA;

        }
    }
}
