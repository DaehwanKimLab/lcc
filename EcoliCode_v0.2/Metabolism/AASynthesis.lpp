// Amino acid Synthesis

from MetaboliteConcentrations import *;

pathway AASynthesis
{
    membrane CellMembrane
    {
        cytosol Cytosol
        {
            float rate_as = EcoliInfo.ProteinSynthesisRate * EcoliInfo.C2M;

            reaction AASynthesis( -> glutamine)
            specification
            {
                glutamine' = rate_as;
            }
        }
    }
}
