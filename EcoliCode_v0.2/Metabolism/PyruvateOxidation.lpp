// Pyruvate sOxidation

import EcoliInfo;
import MetaboliteConcentrations;
import * from MetaboliteConcentrations;

reaction PyruvateOxidation(pyruvate + NAD + CoA_SH -> acetyl_CoA + NADH)
specification
{
    acetyl_CoA' = pyruvate / (MetaboliteConcentrations.pyruvate + pyruvate) * EcoliInfo.EGM_Pyruvate_Sec;
}
