// Fermentation

import AcetateFormation;
import FormateFormation;
import LactateFormation;
import EthanolFormation;

pathway Fermentation
{
    AcetateFormation = AcetateFormation.AcetateFormation;
    FormateFormation = FormateFormation.FormateFormation;
    LactateFormation = LactateFormation.LactateFormation;
    EthanolFormation = EthanolFormation.EthanolFormation;
}

pyruvate = 0.39mM;
