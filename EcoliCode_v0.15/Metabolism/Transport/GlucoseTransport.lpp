// Glucose Transport

reaction GlucoseTransport(glucose{out} + ATP -> G6P{in} + ATP, k=1)
specification
{
    float cgt = 0.5 M;
    G6P' = glucose/G6P * cgt;
}

glucose{out}[:] = 0.15e-6 M;
G6P{in} = 8.8e-3 M;
ATP = 9.6e-3 M;
ADP = 5.6e-4 M;
