// Donghoon M. Lee
// Transcription

import SigmaFactor
import RNAP
import Rho

pathway Transcription 
{
    // Sigma factors for initial RNAP recruitment to the promoter
    sigmafactor SigmaFactor.sigmafactor;
    
    // RNA polymerase for transcription
    polymerase RNAPCore = RNAP.RNAPCore;
    copmlex RNAPHolo = RNAP.RNAPHolo;
    
    // Rho-dependent termination for an additional termination mechanism
    protein Rho = Rho.Rho;
    complex RC, RC_RNA = Rho.RC, Rho.RC_RNA;
}
