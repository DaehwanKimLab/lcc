// Transcription

import SigmaFactor
import RNAP
// import Rho

process Transcription 
{
    DNA Promoter = sigmafactor.consensusbindingsite;
    
    polymerase p(RNAP.RNAPCore); 
    initiation i(transcriptionstartsite);
    termination t(stopcodons); // temporary termination mechanism
    
    complex RNAPHolo(RNAPCore + sigma_factor + Promoter, Kd=1e-10;);
}
