// MixedAcidFermentation_IV
// input: phosphoenolpyruvate
// intermediate: ['pyruvate']
// output: formate
// DB: EcoCyc
// - FERMENTATION-PWY
// Link: https://biocyc.org/ECOLI/NEW-IMAGE?object=FERMENTATION-PWY
pathway MixedAcidFermentation_IV
{
    reaction phosphoenolpyruvate2pyruvate(phosphoenolpyruvate --> pyruvate);
    reaction pyruvate2formate(pyruvate --> formate);
}
