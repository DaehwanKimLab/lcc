// Donghoon M. Lee, Austin Marckx, and Daehwan Kim
// Glycolysis

pathway Glycolysis(G6P + 2 ADP + 2 NAD+ + 2 Pi --> 2 pyruvate + 2 NADH + 2 ATP)
mathfunc
{
    float cg = 10nmol;
    ATP'  = (ADP / ATP) * cg;
    ADP'  = Pi' = - ATP';
    NADH' = ATP';
    NAD+' = - NADH';
    G6P'  = ATP' / 2;
    pyruvate' = G6P' * 2;
}
{
    domain A;
}
