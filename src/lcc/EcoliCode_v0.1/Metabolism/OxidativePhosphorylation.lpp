// ATPSynthase

// temporary code with limited parsing
reaction OxidativePhosphorylation(NADH + FADH2 + 4 ADP --> NAD + FAD + 4 ATP)
specification
{
    float c_co = 5 M;
    float co = FADH2 * c_co;
    ATP'  = co;
    ADP'  = - ATP';
    NADH' = - ATP' / 4;
    FADH2' = - ATP' / 4;
    NAD' = - NADH';
    FAD' = - FADH2';
}

FAD = 1.7e-4 M;
NAD = 2.6e-3 M;
ADP = 5.6e-4 M;
NADH = 8.3e-3 M;
FADH2 = 5e-4 M;
ATP = 9.6e-3 M;
