// ATPSynthase

pathway OxidativePhosphorylation(NADH + FADH2 + ADP --> NAD+ + FAD + 4 ATP)
func
{
    float co = 0.1uM;
    ATP'  = (ADP / ATP) * co;
    ADP'  = - ATP';
    NADH' = FADH2' = ATP' / 4;
    NAD+' = - NADH';
    FAD' = - FADH2';
    acetyl-CoA' = - ATP';
    CoA-SH' = - acetyl-CoA'

}
{

}
