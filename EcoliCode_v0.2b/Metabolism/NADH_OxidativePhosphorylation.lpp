// NADH Oxidative Phosphorylation

from MetaboliteConcentrations import *;

import MetaboliteConcentrations;
import EcoliInfo;

pathway NADH_OxidativePhosphorylation(2 NADH + 5 ADP -> 2 NAD + 5 ATP)
specification
{
    float capacityfactor = EcoliInfo.EGM_OxidativePhosphorylation_NADH_Sec;
    ATP' = NADH / (MetaboliteConcentrations.NADH + NADH) * capacityfactor;
}
