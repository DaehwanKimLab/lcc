// Glucose Transport

reaction GlucoseTransport(Glucose{out} -> G6P{in})
specification
{
    float cgt = 0.5M;
    G6P' = Glucose/G6P * cgt;
    Glucose' = - G6P';
}

Glucose{out} = 9.2M;
G6P = 8.8mM;
