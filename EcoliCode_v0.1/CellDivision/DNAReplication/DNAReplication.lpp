// DNA Replication

import DNAPolymerase;

DnaA = DNAPolymerase.DnaA;
Tus = DNAPolymerase.Tus;
DNAP = DNAPolymerase.DNAP;

pathway DNAReplication 
{
    reaction Initiation
    {
        DnaA_OriBinding = DnaA.OriBinding;
        DNAP_Complexation = DNAP.Complexation;
        DNAP_DNABinding = DNAP.DNABinding;
    }

    reaction Elongation
    {
        dsDNAUnwinding = DNAP.Elongation.dsDNAUnwinding;
        DNATranslocation = DNAP.Elongation.DNATranslocation;
        DNAElongation = DNAP.Elongation.DNAElongation;
        OligoSynthesis = DNAP.OligoSynthesis;
    }
    
    reaction Termination
    {
        Tus_TerBinding = Tus.TerBinding;
        DNAP_Termination = DNAP.Termination;
    }

    DNAP = 40;
    DnaA = 134;
    Tus = 32;
    dNTP[:] = 0.1mM;
}

reference MakiKornberg_1985
{
    title = "The polymerase subunit of DNA polymerase III of Escherichia coli. II. Purification of the alpha subunit, devoid of nuclease activities.";
    doi = "https://doi.org/10.1016/S0021-9258(17)38825-7";
}
